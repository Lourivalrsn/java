import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        System.out.println("\"whats\"");
        System.out.println("\ssame\n");
        //java é dificil?

        /*
         *complicado
         * osso
         * duro
         * mole kkkkkk
         */

        int p = 145;
        System.out.println("my mumber is: " + p);

        long b = 312123313131323133L;
        System.out.println("my mumber is: " + b);

        double i = 3.12;
        System.out.println("my mumber is: " + i);

        boolean g = true;
        System.out.println("my mumber is: " + g);

        char simbolo = '@';
        System.out.println("my mumber is: " + simbolo);

        String name = "bro";
        System.out.println("my mumber is: " + name);

        String x = "agua";
        String y = "suco de uva";
        String temp;

        temp = y;
        y = x;
        x = temp;

        System.out.println("x: " + x);
        System.out.println("y: " + y);

        Scanner Scanner = new Scanner(System.in);
        System.out.println("entre com o seu nome: ");
        String nome = Scanner.nextLine();


        System.out.println("qual sua idade: ");
        int idade = Scanner.nextInt();
        System.out.println("sua idade: " + idade);
        Scanner.nextLine();
        System.out.println("qual sua comida favorita: ");
        String favorita = Scanner.nextLine();

        System.out.println("ola: " + nome);

        System.out.println("você gosta: " + favorita);

        int amigos = 10;

        amigos = amigos + 1;
        System.out.println(amigos);

        int colegas = 20;

        colegas = colegas - 15;
        System.out.println(colegas);

        int familia = 8;
        familia = familia / 2;
        System.out.println(familia);

        int primos = 20;
        primos = primos * 5;
        System.out.println(primos);

        int bateria = 100;
        bateria = bateria % 6;
        System.out.println(bateria);

        double pizza = 12;
        pizza = pizza / 3;
        System.out.println(pizza);
        {

        }

    }
}


